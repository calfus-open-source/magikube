{
  "name": "magikube",
  "description": "CLI tool to bring up infrastructure and basic project",
  "version": "0.0.0",
  "author": "neelshah19",
  "bin": {
    "magikube": "./bin/run.js"
  },
  "bugs": "https://github.com/infrastructure/infrastructure/issues",
  "dependencies": {
    "@oclif/core": "^3.26.6",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@octokit/auth-basic": "^1.4.8",
    "@octokit/rest": "^20.1.1",
    "chalk": "^5.3.0",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "configstore": "^6.0.0",
    "figlet": "^1.7.0",
    "inquirer": "^9.2.20",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "simple-git": "^3.24.0",
    "touch": "^3.1.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/infrastructure/infrastructure",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "magikube",
    "commands": "./dist/commands",
    "dirname": "magikube",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "binAliases": [
      "magikube"
    ],
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    },
    "flexibleTaxonomy": true,
    "hooks": {
      "command_incomplete": "./dist/hooks/command_incomplete.js"
    }
  },
  "repository": "infrastructure/infrastructure",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
