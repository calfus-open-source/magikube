{
  "name": "magikube",
  "description": "Infrastructure as Code in minutes using magikube",
  "version": "0.0.1",
  "author": "Calfus",
  "bin": {
    "magikube": "./bin/run.js"
  },
  "bugs": "https://github.com/calfus-open-source/magikube/issues",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.582.0",
    "@aws-sdk/client-iam": "^3.577.0",
    "@aws-sdk/client-s3": "^3.577.0",
    "@aws-sdk/client-sts": "^3.577.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@types/uuid": "^9.0.8",
    "chalk": "^5.3.0",
    "clear": "^0.1.0",
    "figlet": "^1.7.0",
    "fs-extra": "^11.2.0",
    "fs-extra-plus": "^0.6.0",
    "inquirer": "^9.2.21",
    "js-yaml": "^4.1.0",
    "liquidjs": "^10.13.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/clear": "^0.1.4",
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "copyfiles": "^2.4.1",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/calfus-open-source/magikube",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "magikube",
    "dirname": "magikube",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "https://github.com/calfus-open-source/magikube",
  "scripts": {
    "copy-templates": "copyfiles -u 1 'src/templates/**/*' 'dist'",
    "build": "shx rm -rf dist && tsc -b && npm run copy-templates",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
