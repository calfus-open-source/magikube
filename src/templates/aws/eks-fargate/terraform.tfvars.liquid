region = "{{ aws_region }}"
project_id = "{{ project_id }}"
project_name = "{{ project_name }}"
environment = "{{ environment }}"
vpc_cidr = "{{ aws_vpc_cidr }}"
source_code_repository = "{{ source_code_repository }}"
cluster_version = "{{ aws_eks_cluster_version }}"
{% if source_code_repository == 'github' %}
github_access_token = "{{ github_access_token }}"
github_owner = "{{ github_owner }}"
appType = ["{{ frontend_app_type }}", "{{ backend_app_type }}", "keycloak-service", "auth-service"]
{% endif %}
{% if domain %}
domain_name = "{{ domain }}"
{% endif %}
{% if source_code_repository == 'codecommit' %}
 appType = ["{{ frontend_app_type }}", "{{ backend_app_type }}"]
{% endif %}
